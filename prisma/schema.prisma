generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tasks {
  id       String  @id @db.VarChar(20)
  title    String?
  status   String? @db.VarChar(20)
  label    String? @db.VarChar(20)
  priority String? @db.VarChar(20)
}
